bool checkRecord(char* s) {
    int a = 0; 

    for (int i = 0; i < strlen(s); i++) {
        if (s[i] == 'A') {
            a++;
      if (a >= 2) {
        return false;
      }
    }

    if (i <= strlen(s) && s[i] == 'L' && s[i + 1] == 'L' && s[i + 2] == 'L') {
        return false; 
    }

}
return true;
}
